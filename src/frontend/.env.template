# API keys for Meta Ads (from .env.example)
API_KEY=your_api_key
API_SECRET=your_api_secret

# Database connection (from server/.env.db and server/.env.server)
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=dashboard
DATABASE_URL=postgresql://user:password@localhost:5432/dashboard # Adjusted for native deployment

# JWT Secret (from server/.env.server)
JWT_SECRET=your-jwt-secret

# Other configuration (from .env.example and index.ts)
NODE_ENV=development
PORT=3001 # Default port for the backend
